site_name: {% if meta('project.name') %}{{meta('project.name')}} architecture{% else %}Architecture{% endif %} documentation
pages:
  - index.md{% for f in custom_doc_files %}
  - {{f}}{% endfor %}
  - requirements.md
  - Reference:
      - overview.md
      {% for k,rp in details %}- {{rp.page}}
      {% endfor %}
extra_css:
  - css/light_toc.css
markdown_extensions:
  - admonition
  - toc:
      permalink: true